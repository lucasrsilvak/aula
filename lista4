}

// o)
int crescente(int x, int y) {
	printf("%d ",y);
	if (x > y) 	   return crescente(x, y+1);
	else printf("\n"); return 1;
}

int main()
{
	crescente(5, 0);
	printf("resto: %d\n", res(-699, 400));
	printf("raiz %.2f", raiz(4,2));
}
